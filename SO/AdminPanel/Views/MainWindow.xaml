 <Window x:Class="AdminPanel.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdminPanel.Views"
        xmlns:view="clr-namespace:AdminPanel.Views"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        mc:Ignorable="d"
        Title="Admin Panel - Stack Overflow"
        Height="650" Width="1100"
        MinHeight="650" MinWidth="1100">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <view:UserNavigationView DataContext ="{Binding UserNavigationViewModel}"/>
        <view:UserDetailView DataContext ="{Binding UserDetailViewModel}" Grid.Column="1"/>
    </Grid>

</Window>
