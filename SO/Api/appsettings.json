{
  "ConnectionStrings": {
    "SO_Database": "",
    "SO_ReadonlyDatabase": ""
  },
  "DatabaseOptions": {
    "CommandTimeoutInSeconds": 0,
    "EnableDatailedErrors": false,
    "EnableSensitiveDataLogging": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedSite": "http://localhost:3000"
  },
  "PredictionApiSettings": {
    "Url": "https://localhost:7136"
  },
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "DefaultIndexName": "posts_index"
  },
  "FeatureFlags": {
    "BasicFlag": true,
    "PercentageFlag": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "RolloutPercentageTargetingFlag": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Targeting",
          "Parameters": {
            "Audience": {
              "Users": [
                "U1",
                "U2",
                "U3"
              ],
              "Groups": [
                {
                  "Name": "G1",
                  "RolloutPercentage": 10
                },
                {
                  "Name": "G2",
                  "RolloutPercentage": 50
                },
                {
                  "Name": "G3",
                  "RolloutPercentage": 100
                }
              ],
              "DefaultRolloutPercentage": 0,
              "Exclusion": {
                "Users": [
                  "U4"
                ],
                "Groups": [
                  "G4"
                ]
              }
            }
          }
        }
      ]
    },
    "TimeWindowFlag": {
      "EnabledFor": [
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "Tue, 26 Sep 2023 00:00:00 GMT",
            "End": "Tue, 27 Sep 2024 00:00:00 GMT"
          }
        },
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "Tue, 29 Sep 2024 00:00:00 GMT"
          }
        }
      ]
    }

  }
}